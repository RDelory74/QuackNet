    <!DOCTYPE html>
    <html>
        <head>
            <meta charset="UTF-8">
            <title>{% block title %}Welcome!{% endblock %}</title>
            <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
            <link href="{{ asset('styles/app.css') }}" rel="stylesheet">
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggmGXrrJJjcInscPd6Lh46p9q5NABxMw1u3GmZ9EARg38q5KIKtBT+" crossorigin="anonymous">
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-JNki39SkR9dIczBLKfZ4YPbW/eNKDRIHAVXDSFa5FbwDwpIm5zR8Ag5W1sI2qw/j" crossorigin="anonymous"></script>

            {% block stylesheets %}
            {% endblock %}

            {% block javascripts %}
                {% block importmap %}{{ importmap('app') }}{% endblock %}
            {% endblock %}

        </head>
        <header>

            {% if app.user %}
                <a href="{{ path('app_quack_index') }}">Home</a>
                <a href="{{ path('app_duck_show') }}">Dashboard</a>
                <a href="{{ path('app_logout') }}">Logout</a>
            {% else %}
                <a href="{{ path('app_register') }}">Register</a>
                <a href="{{ path('app_login') }}">Login</a>
            {% endif %}

        </header>

        <body>
        {% block myContent %}{% endblock %}
        </body>
        <footer>
            @2024 by Rod
        </footer>
    </html>
